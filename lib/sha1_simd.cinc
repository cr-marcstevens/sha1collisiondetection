	ihv[0] = SIMD_ADD_VV(ihv[0],a); 
	ihv[1] = SIMD_ADD_VV(ihv[1],b); 
	ihv[2] = SIMD_ADD_VV(ihv[2],c); 
	ihv[3] = SIMD_ADD_VV(ihv[3],d); 
